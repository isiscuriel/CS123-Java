public class Person { 

private String name;

public Person(){
 name ="No name yet";
}
public Person(String intialName){
 name = intialName;
}
public setName(String newName){
 name = newName;
}
public getName(){
return name;
}
public void writeOutput() {
 System.out.println("Name: " + name);
}
public boolean hasSameName(Person otherName){
 return this.name.equalsIgnoreCase(otherPerson.name) 
 }

}